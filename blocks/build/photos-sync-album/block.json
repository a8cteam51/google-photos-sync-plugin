{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "google-photos-sync/album",
  "version": "0.1.0",
  "title": "Google Photos Album",
  "category": "widgets",
  "icon": "format-gallery",
  "description": "Display the selected Google Photos album images.",
  "attributes": {
    "columns": {
      "type": "number",
      "default": 3
    }
  },
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "textdomain": "google-photos-sync",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}